<div class="container p-">
    <div class="row">
      <% movieDatas.forEach(movie => { %>
        
        <div class="col-md-3 col-sm-3 col-4 col-lg-2">
          <a href="/movie/<%= movie.id %>?titles/<%= encodeURIComponent(movie.title) %>" class="lu-one">
          <div class="canodem  a  d">
            <div class="card-body">
            <% if (movie.background || movie.poster_path) { %>
              <img src="https://image.tmdb.org/t/p/w200/<%= movie.background ? movie.background : movie.poster_path %>" class="card" alt="<%= movie.title %> Poster">
            <% } %>
            
              <i class="fas fa-video"></i> <strong class="card-titl"><%= movie.title %></strong>
            </div>
             </a>
           
          </div>
        </div>
      <% }); %>

